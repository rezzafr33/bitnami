    ServerName DOMAIN.dev
    ServerAlias www.DOMAIN.dev
    DocumentRoot "INSTALL_DIR/apps/APP_FOLDER/htdocs/web"
    
    Include "INSTALL_DIR/apps/APP_FOLDER/conf/httpd-app.conf"
</VirtualHost>

